dbSelectArea("SX3")
dbSetOrder(1)
dbSeek("SRA")
While ( !Eof() .And. SX3->X3_ARQUIVO == "SRA" )
	uCampo := SX3->X3_CAMPO
	IF ( SX3->X3_CONTEXT == "V" )
		M->&(uCampo) := CriaVar(SX3->X3_CAMPO)
	Else
		M->&(uCampo) := SRA->( FieldGet(FieldPos(uCampo)) )
	EndIf
	dbSelectArea("SX3")
	dbSkip()
EndDo


SET CENTURY ON

