	If !(M->Z5_DescVal > aCols[n,GdFieldPos("Z5_TOTAL")])
		aCols[n,GdFieldPos("Z5_DESCPER")] := 0
		aCols[n,GdFieldPos("Z5_TOTAL")] := Round( Round(aCols[n,GdFieldPos("Z5_QUANT")] * aCols[n,GdFieldPos("Z5_PRECO")],2) - M->Z5_DESCVAL ,2)
	Else
