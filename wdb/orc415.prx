dbSelectArea("SCJ")
dbSetOrder(6)

If SCJ->( dbSeek( xFilial("SZ4") + Alltrim(cCodJOB) ) )
	//foi gerado pedido de venda, excluir pedido e itens
	dbSelectArea("SCK")
	dbSetOrder(1)
	SCK->(dbSeek(xFilial("SCJ")+SCJ->CJ_NUM))
	
	While !EOF() .AND. SCJ->CJ_NUM == SCK->CK_NUM
		Reclock("SCK",.F.)
