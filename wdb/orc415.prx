		nTotal := Round( Round(aCols[n,GdFieldPos("Z5_QUANT")] * aCols[n,GdFieldPos("Z5_PRECO")],2) - ( Round(aCols[n,GdFieldPos("Z5_QUANT")] * aCols[n,GdFieldPos("Z5_PRECO")],2) / (100/aCols[n,GdFieldPos("Z5_DESCPER")]) ) ,2)
