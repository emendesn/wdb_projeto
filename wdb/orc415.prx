User Function oDescPerc()
Local lRet		:= .T.
Local nTotal	:= 0

If Positivo(M->Z5_DESCPER)
	aCols[n,GdFieldPos("Z5_DESCVAL")] := 0
	If M->Z5_DescPer == 0
		aCols[n,GdFieldPos("Z5_TOTAL")] := Round( aCols[n,GdFieldPos("Z5_QUANT")] * aCols[n,GdFieldPos("Z5_PRECO")] ,2)
	Else
		If M->Z5_DescPer <= 100
			aCols[n,GdFieldPos("Z5_TOTAL")] := Round( Round(aCols[n,GdFieldPos("Z5_QUANT")] * aCols[n,GdFieldPos("Z5_PRECO")],2) - ( Round(aCols[n,GdFieldPos("Z5_QUANT")] * aCols[n,GdFieldPos("Z5_PRECO")],2) / (100/M->Z5_DESCPER) ) ,2)
		Else
		    MsgStop(OemToAnsi("O Percentual de desconto não pode ser maior do que 100"))
		    lRet := .F.
		EndIf
	EndIf
Else
	MsgStop("Desconto Incorreto")
	lRet := .F.
EndIf

Return(lRet)

