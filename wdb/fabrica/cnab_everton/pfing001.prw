/*/
STATIC FUNCTION _fvalor( _cCodBar )

LOCAL _cValor :=""
LOCAL _nTam   := LEN( ALLTRIM( _cCodBar ) )

	IF _nTam == 44
    	_cValor := STRZERO(VAL(SUBSTR(_cCodBar,6,14)),14)                                                                    
	ELSEIF _nTam == 47
		_cValor := SUBSTR(_cCodBar,34,14) 
	ENDIF

RETURN(_cValor)       

