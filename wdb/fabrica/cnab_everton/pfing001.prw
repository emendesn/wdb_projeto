USER FUNCTION _NumAGCTA()

LOCAL _cAGCTA := ""
LOCAL _cConta := ALLTRIM( SA2->A2_NUMCON )

	IF ALLTRIM(SA2->A2_BANCO) == "341"
		_cAGCTA := "0"+SUBST(SA2->A2_AGENCIA,1,4) +" 0000000"+SUBSTR(SA2->A2_NUMCON,1,5)+" "+RIGHT(ALLTRIM(SA2->A2_NUMCON),1)
	ELSE
		_cAGCTA := STRZERO(VAL(SA2->A2_AGENCIA),5) +" "+STRZERO(VAL(SUBSTR(_cConta,1,LEN(_cConta)-1)),12)+" "+RIGHT(_cConta,1)
	ENDIF
		
RETURN(_cAGCTA )

