STATIC FUNCTION _livre( _cCodBar )

LOCAL _cLivre :="" 
LOCAL _nTam   := LEN( ALLTRIM( _cCodBar ) )

	If _nTam == 44
		_cLivre := SUBSTR(_cCodBar,20,25)
	ElseIf _nTam == 47
		_cLivre := SUBSTR(_cCodBar,5,5) +SUBSTR(_cCodBar,11,10)+SUBSTR(_cCodBar,22,10)
	EndIf
	
RETURN( _cLivre )
