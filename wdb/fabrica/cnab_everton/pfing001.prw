STATIC FUNCTION _DV( _cCodBar )

LOCAL _cDV  := ""
LOCAL _nTam := LEN( ALLTRIM( _cCodBar ) )

	IF _nTam == 44
		_cDV := SUBSTR(_cCodBar,5,1)
		//  	Alert(_cDV)
	ELSEIF _nTam == 47
		_cDV := SUBSTR(_cCodBar,33,1)
		//	Alert(_cDV)
	ENDIF
	//	Alert(_cCodBar)
	
RETURN( _cDV )


